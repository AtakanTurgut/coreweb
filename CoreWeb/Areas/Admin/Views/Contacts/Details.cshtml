@model Entities.Contact

@{
    ViewData["Title"] = "İletişim Bilgileri";
}

<div>
    <h4>İletişim Bilgisi Detayları</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Name)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Email)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Phone)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Phone)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Message)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Message)
        </dd>
    </dl>
</div>
<div class="form-group">
    <a asp-action="Index" class="btn btn-sm btn-outline"> Form Takip Sayfası</a>
    <a asp-action="Delete" asp-route-id="@Model.Id" class="btn btn-sm btn-danger">Sil</a>
    <a asp-area="" asp-controller="Admin" asp-action="Default" class="btn btn-sm btn-outline">Admin Paneline Dön</a>
</div>